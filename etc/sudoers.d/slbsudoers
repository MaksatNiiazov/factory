ops ALL = (root) NOPASSWD:SETENV: /home/ops/witzenmann-ops/etc/restart.sh
ops ALL = (root) NOPASSWD:SETENV: /home/ops/witzenmann-ops/etc/stop.sh
ops ALL = (root) NOPASSWD:SETENV: /home/ops/witzenmann-ops/etc/publish.sh
